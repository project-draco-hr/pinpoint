{
  ZooKeeper zookeeper=null;
  try {
    zookeeper=new ZooKeeper(hostPort,sessionTimeout,manager);
    return zookeeper;
  }
 catch (  IOException e) {
    if (zookeeper != null) {
      try {
        zookeeper.close();
      }
 catch (      InterruptedException ie) {
        Thread.currentThread().interrupt();
      }
    }
  }
  return null;
}
