{
  StringBuilder sb=new StringBuilder("Destination(");
  boolean first=true;
  if (isSetId()) {
    sb.append("id:");
    if (this.id == null) {
      sb.append("null");
    }
 else {
      sb.append(this.id);
    }
    first=false;
  }
  if (isSetHost()) {
    if (!first)     sb.append(", ");
    sb.append("host:");
    if (this.host == null) {
      sb.append("null");
    }
 else {
      sb.append(this.host);
    }
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
