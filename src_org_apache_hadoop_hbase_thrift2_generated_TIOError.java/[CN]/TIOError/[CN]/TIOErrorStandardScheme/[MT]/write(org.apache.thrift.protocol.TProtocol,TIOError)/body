{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.message != null) {
    if (struct.isSetMessage()) {
      oprot.writeFieldBegin(MESSAGE_FIELD_DESC);
      oprot.writeString(struct.message);
      oprot.writeFieldEnd();
    }
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
