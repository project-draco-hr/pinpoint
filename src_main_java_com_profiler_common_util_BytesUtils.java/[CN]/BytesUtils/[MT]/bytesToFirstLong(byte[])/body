{
  if (buf == null) {
    throw new NullPointerException("buf must not be null");
  }
  if (buf.length < 8) {
    throw new IllegalArgumentException("buf.length is too small(8). buf.length:" + buf.length);
  }
  long rv=(((long)buf[0] & 0xff) << 56) | (((long)buf[1] & 0xff) << 48) | (((long)buf[2] & 0xff) << 40)| (((long)buf[3] & 0xff) << 32)| (((long)buf[4] & 0xff) << 24)| (((long)buf[5] & 0xff) << 16)| (((long)buf[6] & 0xff) << 8)| (((long)buf[7] & 0xff));
  return rv;
}
