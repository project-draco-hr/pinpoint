{
  try {
    byte[] bytes=ctClass.toBytecode();
    ctClass.detach();
    return bytes;
  }
 catch (  IOException e) {
    logger.info("IoException class:" + ctClass.getName() + " "+ e.getMessage(),e);
  }
catch (  CannotCompileException e) {
    logger.info("CannotCompileException class:" + ctClass.getName() + " "+ e.getMessage(),e);
  }
  return null;
}
