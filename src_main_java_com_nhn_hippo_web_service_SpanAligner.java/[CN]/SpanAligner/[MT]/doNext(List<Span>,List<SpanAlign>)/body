{
  if (spans == null) {
    return;
  }
  depth++;
  try {
    for (    Span next : spans) {
      if (logger.isDebugEnabled()) {
        logger.debug("{} {} next {}",new Object[]{getSpace(),depth,next});
      }
      result.add(new SpanAlign(depth,next));
      List<Span> nextSpan=nextSpan(next);
      doNext(nextSpan,result);
    }
  }
  finally {
    depth--;
  }
}
