{
  if (isDebug) {
    logger.afterInterceptor(target,args,result);
  }
  final Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    return;
  }
  try {
    trace.recordServiceType(ServiceType.DBCP);
    trace.recordApi(descriptor,null);
    trace.recordException(result);
    trace.markAfterTime();
  }
  finally {
    trace.traceBlockEnd();
  }
}
