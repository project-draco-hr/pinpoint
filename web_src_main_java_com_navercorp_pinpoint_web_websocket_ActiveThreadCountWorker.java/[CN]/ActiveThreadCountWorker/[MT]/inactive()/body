{
synchronized (lock) {
    if (!started && stopped) {
      return;
    }
    stopped=true;
    logger.info("ActiveThreadCountWorker stop. agentId:{}, streamChannel:{}",agentInfo.getAgentId(),streamChannel);
    try {
      streamConnectionManager.removeReconnectJob(agentInfo);
      closeStreamChannel();
    }
 catch (    Exception e) {
    }
  }
}
