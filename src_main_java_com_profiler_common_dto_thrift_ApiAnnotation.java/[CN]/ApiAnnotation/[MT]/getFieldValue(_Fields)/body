{
switch (field) {
case CLASS_NAME:
    return getClassName();
case METHOD_NAME:
  return getMethodName();
case PARAMETER_TYPE:
return getParameterType();
case PARAMETER_NAME:
return getParameterName();
case PARAMETER_VALUE:
return getParameterValue();
case LINE:
return Integer.valueOf(getLine());
}
throw new IllegalStateException();
}
