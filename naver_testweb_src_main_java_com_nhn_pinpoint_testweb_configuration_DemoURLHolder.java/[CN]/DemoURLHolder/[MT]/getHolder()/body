{
  try {
    String hostname=java.net.InetAddress.getLocalHost().getHostName();
    if (hostname == null) {
      return new DemoURLHolderLocal();
    }
    if (hostname.endsWith("nhnsystem.com")) {
      return new DemoURLHolderDev();
    }
 else {
      return new DemoURLHolderLocal();
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    return new DemoURLHolderLocal();
  }
}
