{
  assert(tbase instanceof Span);
  try {
    Span span=(Span)tbase;
    tindex.insert(span);
    trace.insert(span);
    if (logger.isInfoEnabled()) {
      logger.info("Received SPAN=" + span);
    }
  }
 catch (  Exception e) {
    logger.warn("ReadJVMData handle error " + e.getMessage(),e);
  }
}
