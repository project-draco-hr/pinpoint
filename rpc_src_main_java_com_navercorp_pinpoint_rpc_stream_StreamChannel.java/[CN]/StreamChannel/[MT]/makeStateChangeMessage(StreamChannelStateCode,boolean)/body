{
  StringBuilder sb=new StringBuilder();
  sb.append(this.getClass().getSimpleName());
  sb.append(" change state to ");
  sb.append(change.name());
  sb.append("(");
  sb.append(result ? "SUCCESS" : "FAIL");
  sb.append(")");
  sb.append("[Channel:");
  sb.append(channel);
  sb.append(", StreamId:");
  sb.append(getStreamId());
  sb.append(".");
  return sb.toString();
}
