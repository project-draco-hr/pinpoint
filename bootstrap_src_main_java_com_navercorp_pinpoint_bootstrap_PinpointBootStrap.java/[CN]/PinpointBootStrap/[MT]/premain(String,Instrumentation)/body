{
  if (agentArgs != null) {
    logger.info(ProductInfo.NAME + " agentArgs:" + agentArgs);
  }
  final boolean duplicated=checkDuplicateLoadState();
  if (duplicated) {
    logPinpointAgentLoadFail();
    return;
  }
  PinpointBootStrap bootStrap=new PinpointBootStrap(agentArgs,instrumentation);
  bootStrap.start();
}
