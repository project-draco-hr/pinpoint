{
  if (spans == null) {
    return;
  }
  depth++;
  try {
    for (    SpanBo next : spans) {
      if (logger.isDebugEnabled()) {
        logger.debug("{} {} next {}",getSpace(),depth,next);
      }
      result.add(new SpanAlign(depth,next));
      List<SpanBo> nextSpan=nextSpan(next);
      doNext(nextSpan,result);
    }
  }
  finally {
    depth--;
  }
}
