{
  if (args == null) {
    sb.append("null");
    return;
  }
  if (args.length == 0) {
    return;
  }
  if (args.length > 0) {
    sb.append('(');
    sb.append(args[0]);
    for (int i=1; i < args.length; i++) {
      sb.append(", ");
      sb.append(args[i]);
    }
    sb.append(')');
  }
  return;
}
