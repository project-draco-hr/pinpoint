{
  if (args == null) {
    sb.append("()");
    return;
  }
  if (args.length == 0) {
    sb.append("()");
    return;
  }
  if (args.length > 0) {
    sb.append('(');
    sb.append(normalizedParameter(args[0]));
    for (int i=1; i < args.length; i++) {
      sb.append(", ");
      sb.append(normalizedParameter(args[i]));
    }
    sb.append(')');
  }
  return;
}
