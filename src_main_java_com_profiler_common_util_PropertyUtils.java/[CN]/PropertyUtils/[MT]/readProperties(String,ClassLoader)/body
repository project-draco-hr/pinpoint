{
  Properties properties=new Properties();
  InputStream stream=cl.getResourceAsStream(propertyName);
  if (stream == null) {
    throw new FileNotFoundException(propertyName + " not found.");
  }
  try {
    properties.load(stream);
  }
  finally {
    try {
      stream.close();
    }
 catch (    IOException e) {
    }
  }
  return properties;
}
