{
  this.tab=tab;
  this.method=method;
  this.title=title;
  this.arguments=arguments;
  this.begin=begin;
  this.elapsed=elapsed;
  this.agent=agent;
  this.service=service;
  this.serviceType=serviceType;
  this.destinationId=destinationId;
  this.excludeFromTimeline=serviceType == null || serviceType.isInternalMethod();
}
