{
  try {
    Put put=new Put(SpanUtils.getTraceIndexRowKey(span),span.getTimestamp());
    put.add(COLFAM_TRACE,COLNAME_ID,SpanUtils.getTraceId(span));
    client.insert(HBaseTables.TRACE_INDEX,put);
    return true;
  }
 catch (  Exception e) {
    logger.error(e.getMessage(),e);
  }
  return false;
}
