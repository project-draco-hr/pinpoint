{
  boolean debugEnabled=logger.isDebugEnabled();
  if (debugEnabled) {
    logger.debug("Loop is stop.");
  }
  while (true) {
    Collection<Object> dtoList=getDrainQueue();
    int drainSize=takeN(dtoList,this.maxDrainSize);
    if (drainSize == 0) {
      break;
    }
    if (debugEnabled) {
      logger.debug("flushData size {}",drainSize);
    }
    sendPacketN(dtoList);
  }
}
