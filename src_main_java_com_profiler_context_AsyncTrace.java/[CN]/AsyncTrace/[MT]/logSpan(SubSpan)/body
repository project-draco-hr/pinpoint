{
  try {
    if (isDebug) {
      Thread thread=Thread.currentThread();
      logger.info("[WRITE SubSPAN]" + subSpan + " CurrentThreadID="+ thread.getId()+ ",\n\t CurrentThreadName="+ thread.getName());
    }
    this.storage.store(subSpan);
  }
 catch (  Exception e) {
    logger.log(Level.SEVERE,e.getMessage(),e);
  }
}
