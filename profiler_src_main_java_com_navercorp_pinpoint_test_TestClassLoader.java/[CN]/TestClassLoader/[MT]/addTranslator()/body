{
  try {
    ClassPool classPool=agent.getClassPool().getClassPool(this);
    addTranslator(classPool,instrumentTranslator);
  }
 catch (  NotFoundException e) {
    throw new RuntimeException(e.getMessage(),e);
  }
catch (  CannotCompileException e) {
    throw new RuntimeException(e.getMessage(),e);
  }
}
