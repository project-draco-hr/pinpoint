{
  try {
    addTranslator(((JavaAssistByteCodeInstrumentor)instrumentor).getClassPool(this),instrumentTranslator);
  }
 catch (  NotFoundException e) {
    throw new RuntimeException(e.getMessage(),e);
  }
catch (  CannotCompileException e) {
    throw new RuntimeException(e.getMessage(),e);
  }
}
