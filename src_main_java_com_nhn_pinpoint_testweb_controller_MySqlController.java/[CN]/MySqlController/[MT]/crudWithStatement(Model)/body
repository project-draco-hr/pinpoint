{
  try {
    int id=(new Random()).nextInt();
    Member member=new Member();
    member.setId(id);
    member.setName("chisu");
    member.setJoined(new Date());
    service.addStatement(member);
    service.list();
    service.delete(id);
    return "OK";
  }
 catch (  Exception e) {
    logger.error(e.getMessage(),e);
    return e.getMessage();
  }
}
