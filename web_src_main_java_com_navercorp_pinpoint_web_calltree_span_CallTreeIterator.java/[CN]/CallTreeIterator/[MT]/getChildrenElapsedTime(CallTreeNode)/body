{
  long elapsedTime=0;
  CallTreeNode child=node.getChild();
  while (child != null) {
    SpanAlign align=child.getValue();
    if (align.isSpan() || (align.isAsync() && align.getSpanEventBo().getSequence() == 0)) {
    }
 else {
      elapsedTime+=align.getSpanEventBo().getEndElapsed();
    }
    child=child.getSibling();
  }
  return elapsedTime;
}
