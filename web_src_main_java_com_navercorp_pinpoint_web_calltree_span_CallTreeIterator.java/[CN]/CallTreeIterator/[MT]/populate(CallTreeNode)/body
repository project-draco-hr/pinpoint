{
  nodes.add(node);
  index++;
  final SpanAlign align=node.getValue();
  align.setGap(getGap());
  align.setDepth(node.getDepth());
  align.setExecutionMilliseconds(getExecutionTime());
  if (node.hasChild()) {
    populate(node.getChild());
  }
  if (node.hasSibling()) {
    populate(node.getSibling());
  }
}
