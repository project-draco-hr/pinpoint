{
  afterPropertiesSet();
  if (socket == null) {
    throw new RuntimeException("socket create fail");
  }
  logger.info("UDP Packet reader:{} started.",ioThreadSize);
  for (int i=0; i < ioThreadSize; i++) {
    io.execute(new Runnable(){
      @Override public void run(){
        receive();
      }
    }
);
  }
}
