{
  final int sequence=spanEventBo.getSequence();
  if (isAsyncEvent(spanEventBo)) {
    if (async.first) {
      if (sequence != 0) {
        return true;
      }
    }
 else {
      if (sequence != 0 && sequence > async.sequence + 1) {
        return true;
      }
    }
  }
 else {
    if (sequence > sync.sequence + 1) {
      return true;
    }
  }
  return false;
}
