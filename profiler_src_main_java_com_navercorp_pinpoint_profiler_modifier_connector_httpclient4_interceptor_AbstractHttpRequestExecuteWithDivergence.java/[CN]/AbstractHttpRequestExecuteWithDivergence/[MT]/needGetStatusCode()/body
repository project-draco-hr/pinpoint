{
  if (isHasCallbackParam) {
    return false;
  }
  final Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    return false;
  }
  if (trace.getServiceType() != ServiceType.HTTP_CLIENT.getCode()) {
    return false;
  }
  if (scope.getAttachment() != null) {
    return false;
  }
  return true;
}
