{
switch (field) {
case TRACE_ID:
    return getTraceID();
case NAME:
  return getName();
case SPAN_ID:
return getSpanID();
case PARENT_SPAN_ID:
return getParentSpanId();
case ANNOTATIONS:
return getAnnotations();
case BINARY_ANNOTATIONS:
return getBinaryAnnotations();
case DEBUG:
return Boolean.valueOf(isDebug());
}
throw new IllegalStateException();
}
