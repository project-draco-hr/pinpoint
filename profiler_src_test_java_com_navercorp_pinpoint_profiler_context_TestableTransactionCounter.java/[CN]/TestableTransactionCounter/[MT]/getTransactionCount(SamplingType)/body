{
switch (samplingType) {
case SAMPLED_NEW:
    return this.sampledTransactionCount;
case UNSAMPLED_NEW:
  return this.unsampledTransactionCount;
case SAMPLED_CONTINUATION:
return this.sampledContinuationCount;
case UNSAMPLED_CONTINUATION:
return this.unsampledContinuationCount;
default :
return 0L;
}
}
