{
  StringBuilder builder=new StringBuilder();
  builder.append("{beginPosition=");
  builder.append(beginPosition);
  builder.append(", endPosition=");
  builder.append(endPosition);
  builder.append(", className=");
  builder.append(className);
  builder.append(", size=");
  builder.append(size());
  builder.append("}");
  return builder.toString();
}
