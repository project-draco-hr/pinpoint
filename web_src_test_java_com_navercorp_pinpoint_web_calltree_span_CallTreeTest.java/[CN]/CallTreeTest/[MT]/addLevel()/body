{
  CallTree callTree=new CallTree(null);
  expectResult.clear();
  expectResult.add("#");
  expectResult.add("##");
  expectResult.add("###");
  expectResult.add("####");
  expectResult.add("#####");
  expectResult.add("#####");
  expectResult.add("#####");
  expectResult.add("#####");
  callTree.add(1,null);
  callTree.add(2,null);
  callTree.add(3,null);
  callTree.add(4,null);
  callTree.add(-1,null);
  callTree.add(-1,null);
  callTree.add(-1,null);
  assertDepth("addLevel",callTree,expectResult);
}
