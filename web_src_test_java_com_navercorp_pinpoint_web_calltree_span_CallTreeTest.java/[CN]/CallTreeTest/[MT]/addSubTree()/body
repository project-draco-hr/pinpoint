{
  CallTree callTree=new CallTree(null);
  expectResult.clear();
  expectResult.add("#");
  expectResult.add("##");
  expectResult.add("###");
  expectResult.add("##");
  callTree.add(1,null);
  CallTree subTree=new CallTree(null);
  callTree.add(subTree);
  callTree.add(-1,null);
  assertDepth("addSubTree",callTree,expectResult);
}
