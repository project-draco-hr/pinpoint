{
  if (listener == null) {
    throw new NullPointerException("listener");
  }
  boolean alreadyReady=false;
synchronized (this) {
    if (ready) {
      alreadyReady=true;
    }
 else {
      this.listeners.add(listener);
    }
  }
  if (alreadyReady) {
    fireOnComplete(listener);
  }
  return !alreadyReady;
}
