{
  if (data == null) {
    logger.warn("putQueue(). data is null");
    return false;
  }
  if (!allowInput.get()) {
    logger.warn("datasender is shutdown. discard data:{}",data);
    return false;
  }
  boolean offer=queue.offer(data);
  if (!offer) {
    if (logger.isWarnEnabled()) {
      logger.warn("Drop data. queue is full. size:{}",queue.size());
    }
  }
  return offer;
}
