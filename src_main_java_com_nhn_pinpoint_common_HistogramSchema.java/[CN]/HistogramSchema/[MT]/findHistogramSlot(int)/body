{
  if (elapsedTime == ERROR_SLOT_TIME) {
    return errorSlot;
  }
  if (elapsedTime <= this.fastSlot.getSlotTime()) {
    return fastSlot;
  }
  if (elapsedTime <= this.normalSlot.getSlotTime()) {
    return normalSlot;
  }
  if (elapsedTime <= this.slowSlot.getSlotTime()) {
    return slowSlot;
  }
  return verySlowSlot;
}
