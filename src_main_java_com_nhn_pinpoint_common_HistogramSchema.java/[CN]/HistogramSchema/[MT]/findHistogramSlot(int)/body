{
  if (elapsedTime == ERROR_SLOT_TIME) {
    return errorSlot;
  }
  if (this.fastSlot.getSlotTime() <= elapsedTime) {
    return fastSlot;
  }
  if (this.normalSlot.getSlotTime() <= elapsedTime) {
    return normalSlot;
  }
  if (this.slowSlot.getSlotTime() <= elapsedTime) {
    return slowSlot;
  }
  return verySlowSlot;
}
