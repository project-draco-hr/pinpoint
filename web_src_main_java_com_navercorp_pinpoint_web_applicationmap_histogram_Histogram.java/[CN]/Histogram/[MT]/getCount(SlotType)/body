{
  if (slotType == null) {
    throw new NullPointerException("slotType must not be null");
  }
switch (slotType) {
case FAST:
    return fastCount;
case NORMAL:
  return normalCount;
case SLOW:
return slowCount;
case VERY_SLOW:
return verySlowCount;
case ERROR:
return errorCount;
}
throw new IllegalArgumentException("slotType:" + slotType);
}
