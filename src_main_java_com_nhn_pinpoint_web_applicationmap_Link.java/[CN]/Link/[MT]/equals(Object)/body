{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Link other=(Link)obj;
  if (linkKey == null) {
    if (other.linkKey != null)     return false;
  }
 else   if (!linkKey.equals(other.linkKey))   return false;
  return true;
}
