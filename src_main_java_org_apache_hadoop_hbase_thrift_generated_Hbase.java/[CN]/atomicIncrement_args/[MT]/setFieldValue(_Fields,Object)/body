{
switch (field) {
case TABLE_NAME:
    if (value == null) {
      unsetTableName();
    }
 else {
      setTableName((ByteBuffer)value);
    }
  break;
case ROW:
if (value == null) {
  unsetRow();
}
 else {
  setRow((ByteBuffer)value);
}
break;
case COLUMN:
if (value == null) {
unsetColumn();
}
 else {
setColumn((ByteBuffer)value);
}
break;
case VALUE:
if (value == null) {
unsetValue();
}
 else {
setValue((Long)value);
}
break;
}
}
