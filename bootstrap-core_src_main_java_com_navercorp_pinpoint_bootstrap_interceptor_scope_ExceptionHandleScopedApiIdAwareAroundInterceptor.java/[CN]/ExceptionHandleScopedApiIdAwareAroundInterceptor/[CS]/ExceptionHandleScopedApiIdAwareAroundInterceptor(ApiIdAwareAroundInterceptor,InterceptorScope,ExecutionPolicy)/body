{
  if (delegate == null) {
    throw new NullPointerException("delegate must not be null");
  }
  if (scope == null) {
    throw new NullPointerException("scope must not be null");
  }
  if (policy == null) {
    throw new NullPointerException("policy must not be null");
  }
  this.delegate=delegate;
  this.scope=scope;
  this.policy=policy;
}
