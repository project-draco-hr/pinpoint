{
  Assert.assertTrue(AntPathMatcher.isAntStylePattern("/*/test"));
  Assert.assertTrue(AntPathMatcher.isAntStylePattern("/*/?"));
  Assert.assertTrue(AntPathMatcher.isAntStylePattern("*.test"));
  Assert.assertTrue(AntPathMatcher.isAntStylePattern("*.?"));
  Assert.assertFalse(AntPathMatcher.isAntStylePattern("/abc/test"));
  Assert.assertFalse(AntPathMatcher.isAntStylePattern("abc.test"));
}
