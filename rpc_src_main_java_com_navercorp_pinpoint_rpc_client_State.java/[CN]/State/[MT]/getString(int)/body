{
switch (stateCode) {
case INIT:
    return "INIT";
case RUN:
  return "RUN";
case RUN_DUPLEX_COMMUNICATION:
return "RUN_DUPLEX_COMMUNICATION";
case RUN_SIMPLEX_COMMUNICATION:
return "RUN_SIMPLEX_COMMUNICATION";
case CLOSED:
return "CLOSED";
case RECONNECT:
return "RECONNECT";
case INIT_RECONNECT:
return "INIT_RECONNECT";
}
return "UNKNOWN";
}
