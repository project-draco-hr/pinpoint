{
  totalCount+=value;
  if (slot == 0) {
    slowCount+=value;
  }
 else   if (slot == -1) {
    errorCount+=value;
    return;
  }
  int histogramSlotIndex=histogram.getHistogramSlotIndex(slot);
  if (histogramSlotIndex == -1) {
    logger.warn("Can't find slot={} value={} id={} serviceType={}",slot,value,id,serviceType);
    return;
  }
  values[histogramSlotIndex]+=value;
}
