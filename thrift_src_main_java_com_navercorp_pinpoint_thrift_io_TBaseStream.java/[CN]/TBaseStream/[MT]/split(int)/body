{
  final List<ByteArrayOutput> list=new ArrayList<ByteArrayOutput>();
  int currentSize=0;
  TBaseStreamNode node=null;
  while ((node=nodes.peek()) != null) {
    if (node.size() > maxSize) {
      if (list.size() == 0) {
        list.add(node);
        nodes.poll();
      }
      break;
    }
    if (currentSize + node.size() > maxSize) {
      break;
    }
    currentSize+=node.size();
    list.add(node);
    nodes.poll();
  }
  return list;
}
