{
  if (from == null) {
    throw new NullPointerException("from must not be null");
  }
  if (to == null) {
    throw new NullPointerException("to must not be null");
  }
  if (id == null) {
    throw new NullPointerException("id must not be null");
  }
  this.id=id;
  this.from=from;
  this.to=to;
  this.hostList=hostList;
}
