{
  assertVar64(127);
  assertVar64(128);
  assertVar64(16383);
  assertVar64(16384);
  assertVar64(2097151);
  assertVar64(2097152);
  assertVar64(268435455);
  assertVar64(268435456);
  assertVar64(34359738367L);
  assertVar64(34359738368L);
  assertVar64(Long.MAX_VALUE - 1);
  assertVar64(Long.MAX_VALUE);
  assertVar64(Long.MIN_VALUE);
  assertVar64(Long.MIN_VALUE + 1);
  assertVar64(-127);
  assertVar64(-128);
  assertVar64(-2097151);
  assertVar64(-2097152);
  assertVar64(-34359738367L);
  assertVar64(-34359738368L);
}
