{
  assertVar32(127);
  assertVar32(128);
  assertVar32(16383);
  assertVar32(16384);
  assertVar32(2097151);
  assertVar32(2097152);
  assertVar32(268435455);
  assertVar32(268435456);
  assertVar32(Integer.MAX_VALUE - 1);
  assertVar32(Integer.MAX_VALUE);
  assertVar32(Integer.MIN_VALUE);
  assertVar32(Integer.MIN_VALUE + 1);
  assertVar32(-127);
  assertVar32(-128);
  assertVar32(-16383);
  assertVar32(-16384);
  assertVar32(-268435455);
  assertVar32(-268435456);
}
