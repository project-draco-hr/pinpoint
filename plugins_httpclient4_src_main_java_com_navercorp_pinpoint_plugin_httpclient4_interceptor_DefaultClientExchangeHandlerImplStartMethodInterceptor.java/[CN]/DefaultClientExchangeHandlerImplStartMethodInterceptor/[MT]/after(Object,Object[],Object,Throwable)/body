{
  if (isDebug) {
    logger.afterInterceptor(target,args);
  }
  final Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    return;
  }
  trace.recordServiceType(HTTP_CLIENT4_INTERNAL);
  trace.recordApi(methodDescriptor);
  trace.recordException(throwable);
  trace.markAfterTime();
  trace.traceBlockEnd();
}
