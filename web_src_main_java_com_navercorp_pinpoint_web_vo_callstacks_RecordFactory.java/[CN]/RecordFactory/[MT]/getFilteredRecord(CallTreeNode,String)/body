{
  final SpanAlign align=node.getValue();
  align.setId(getNextId());
  final int parentId=getParentId(node);
  Api api=getApi(align);
  final Record record=new Record(align.getDepth(),align.getId(),parentId,true,apiTitle,"",align.getStartTime(),align.getElapsed(),align.getGap(),"UNKNOWN",align.getApplicationId(),ServiceType.UNKNOWN,"",false,false,align.getTransactionId(),align.getSpanId(),align.getExecutionMilliseconds(),0,false);
  return record;
}
