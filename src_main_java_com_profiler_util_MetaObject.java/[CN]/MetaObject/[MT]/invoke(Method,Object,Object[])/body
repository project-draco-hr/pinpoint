{
  if (method == null) {
    return defaultReturnValue;
  }
  try {
    return (R)method.invoke(target,args);
  }
 catch (  IllegalAccessException e) {
    logger.log(Level.WARNING,"invoke fail",e);
    return defaultReturnValue;
  }
catch (  InvocationTargetException e) {
    logger.log(Level.WARNING,"invoke fail",e);
    return defaultReturnValue;
  }
}
