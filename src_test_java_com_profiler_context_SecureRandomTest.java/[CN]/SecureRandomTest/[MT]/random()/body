{
  byte[] randomBytes=new byte[16];
  random.nextBytes(randomBytes);
  randomBytes[8]&=0x3f;
  randomBytes[8]|=0x80;
  long msb=0;
  for (int i=0; i < 8; i++) {
    msb=(msb << 8) | (randomBytes[i] & 0xff);
  }
  randomBytes[6]&=0x0f;
  randomBytes[6]|=0x40;
  long lsb=0;
  for (int i=8; i < 16; i++) {
    lsb=(lsb << 8) | (randomBytes[i] & 0xff);
  }
}
