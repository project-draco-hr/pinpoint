{
  if (isDebug) {
    logger.beforeInterceptor(target,args);
  }
  try {
    if (!validate(target,args)) {
      return;
    }
    final String endPoint=endPointAccessor.get(args[0]);
    if (endPoint != null) {
      endPointAccessor.set(target,endPoint);
    }
  }
 catch (  Throwable t) {
    logger.warn("Failed to BEFORE process. {}",t.getMessage(),t);
  }
}
