{
  CyclicStateChecker stateChecker=new CyclicStateChecker(3);
  stateChecker.markAndCheckCondition();
  Assert.assertFalse(stateChecker.checkCondition());
  stateChecker.markAndCheckCondition();
  Assert.assertFalse(stateChecker.checkCondition());
  stateChecker.unmark();
  Assert.assertFalse(stateChecker.checkCondition());
  stateChecker.markAndCheckCondition();
  Assert.assertFalse(stateChecker.checkCondition());
  stateChecker.markAndCheckCondition();
  Assert.assertFalse(stateChecker.checkCondition());
  stateChecker.markAndCheckCondition();
  Assert.assertTrue(stateChecker.checkCondition());
}
