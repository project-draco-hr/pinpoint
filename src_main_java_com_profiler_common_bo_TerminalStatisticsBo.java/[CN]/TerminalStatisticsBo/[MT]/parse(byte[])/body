{
  ByteArrayInputStream bis=null;
  ObjectInput in=null;
  try {
    bis=new ByteArrayInputStream(bytes);
    in=new ObjectInputStream(bis);
    return (TerminalStatisticsBo)in.readObject();
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
 finally {
    try {
      if (in != null)       in.close();
    }
 catch (    Exception e2) {
      e2.printStackTrace();
    }
    try {
      if (bis != null)       bis.close();
    }
 catch (    Exception e2) {
      e2.printStackTrace();
    }
  }
}
