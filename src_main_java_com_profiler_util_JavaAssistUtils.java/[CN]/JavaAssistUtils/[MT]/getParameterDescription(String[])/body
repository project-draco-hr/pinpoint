{
  if (params == null) {
    return EMTPY_ARRAY;
  }
  StringBuilder sb=new StringBuilder(64);
  sb.append('(');
  int end=params.length - 1;
  for (int i=0; i < params.length; i++) {
    sb.append(params[i]);
    if (i < end) {
      sb.append(", ");
    }
  }
  sb.append(')');
  return sb.toString();
}
