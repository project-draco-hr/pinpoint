{
  try {
    if (Trace.getCurrentTraceId() == null) {
      return;
    }
    Trace.recordRpcName("mysql","");
    if (args.length > 0) {
      Trace.recordAttibute("Query",args[0]);
    }
    Trace.record(Annotation.ClientSend);
    StopWatch.start("ExecuteQueryMethodInterceptor");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
