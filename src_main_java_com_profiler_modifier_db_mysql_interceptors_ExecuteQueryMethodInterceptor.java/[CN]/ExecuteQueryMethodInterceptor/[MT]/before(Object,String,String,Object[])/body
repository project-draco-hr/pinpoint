{
  try {
    System.out.println("ThreadID=" + Thread.currentThread().getId());
    Trace.recordRpcName("mysql","");
    if (args.length > 0) {
      Trace.record("Query=" + args[0]);
    }
    Trace.record(new Annotation.ClientSend());
    StopWatch.start("ExecuteQueryMethodInterceptor");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
