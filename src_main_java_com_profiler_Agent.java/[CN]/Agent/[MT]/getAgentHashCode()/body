{
  if (agentHash == 0) {
    String portNumbers="";
    for (    Entry<Integer,String> entry : serverInfo.getConnectors().entrySet()) {
      portNumbers+=" " + entry.getKey();
    }
    portNumbers=portNumbers.trim();
    agentHash=(serverInfo.getHostip() + portNumbers).hashCode();
  }
  return agentHash;
}
