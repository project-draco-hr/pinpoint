{
  if (method != null) {
    try {
      method.addGroupedInterceptor(interceptorClassName,constructorArgs,group);
      return true;
    }
 catch (    InstrumentException e) {
      if (logger.isWarnEnabled()) {
        logger.warn("Unsupported method " + method,e);
      }
    }
  }
  return false;
}
