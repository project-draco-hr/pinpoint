{
  if (args == null) {
    return "null";
  }
  if (args.length == 2) {
    String str=StringUtils.toString(args[0]) + ":" + StringUtils.toString(getClassName(args[1]));
    return StringUtils.drop(str);
  }
 else   if (args.length == 3) {
    String str=StringUtils.toString(args[0]) + ":" + StringUtils.toString(getClassName(args[1]));
    return StringUtils.drop(str);
  }
  return "error";
}
