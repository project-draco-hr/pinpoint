{
switch (type) {
case AROUND:
    addBeforeInterceptor(interceptorId,isStatic);
  addAfterInterceptor(interceptorId,isStatic);
break;
case BEFORE:
addBeforeInterceptor(interceptorId,isStatic);
break;
case AFTER:
addAfterInterceptor(interceptorId,isStatic);
break;
default :
throw new UnsupportedOperationException("unsupported type");
}
}
