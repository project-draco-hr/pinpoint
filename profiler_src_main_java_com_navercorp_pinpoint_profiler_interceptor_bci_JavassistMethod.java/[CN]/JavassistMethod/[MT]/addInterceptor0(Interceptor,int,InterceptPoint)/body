{
switch (point) {
case AROUND:
    addBeforeInterceptor(interceptor,interceptorId);
  addAfterInterceptor(interceptor,interceptorId);
break;
case BEFORE:
addBeforeInterceptor(interceptor,interceptorId);
break;
case AFTER:
addAfterInterceptor(interceptor,interceptorId);
break;
default :
throw new UnsupportedOperationException("unsupported type");
}
}
