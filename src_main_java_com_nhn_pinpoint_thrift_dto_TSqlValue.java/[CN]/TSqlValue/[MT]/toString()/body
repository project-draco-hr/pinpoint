{
  StringBuilder sb=new StringBuilder("TSqlValue(");
  boolean first=true;
  sb.append("id:");
  sb.append(this.id);
  first=false;
  if (isSetBindValue()) {
    if (!first)     sb.append(", ");
    sb.append("bindValue:");
    if (this.bindValue == null) {
      sb.append("null");
    }
 else {
      sb.append(this.bindValue);
    }
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
