{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetId()) {
    optionals.set(0);
  }
  if (struct.isSetBindValue()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetId()) {
    oprot.writeI32(struct.id);
  }
  if (struct.isSetBindValue()) {
    oprot.writeString(struct.bindValue);
  }
}
