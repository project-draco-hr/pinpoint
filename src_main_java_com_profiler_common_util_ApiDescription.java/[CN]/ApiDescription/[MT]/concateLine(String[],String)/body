{
  if (stringList == null || stringList.length == 0) {
    return "()";
  }
  StringBuilder sb=new StringBuilder();
  if (stringList.length > 0) {
    sb.append(stringList[0]);
    for (int i=1; i < stringList.length; i++) {
      sb.append(separator);
      sb.append(stringList[i]);
    }
  }
  return sb.toString();
}
