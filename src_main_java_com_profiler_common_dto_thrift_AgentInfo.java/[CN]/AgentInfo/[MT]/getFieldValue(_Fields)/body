{
switch (field) {
case HOSTNAME:
    return getHostname();
case IP:
  return getIp();
case PORTS:
return getPorts();
case AGENT_ID:
return getAgentId();
case APPLICATION_NAME:
return getApplicationName();
case IS_ALIVE:
return Boolean.valueOf(isIsAlive());
case TIMESTAMP:
return Long.valueOf(getTimestamp());
case IDENTIFIER:
return Short.valueOf(getIdentifier());
}
throw new IllegalStateException();
}
