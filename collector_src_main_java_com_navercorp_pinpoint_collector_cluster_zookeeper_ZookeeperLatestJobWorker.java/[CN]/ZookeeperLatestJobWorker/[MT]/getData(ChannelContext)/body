{
  String uniquePath=getUniquePath(channelContext,false);
  if (uniquePath == null) {
    logger.info("Can't find suitable UniqPath ChannelContext = {}.",channelContext);
    return null;
  }
  try {
    return zookeeperClient.getData(uniquePath);
  }
 catch (  Exception e) {
    logger.warn(e.getMessage(),e);
  }
  return null;
}
