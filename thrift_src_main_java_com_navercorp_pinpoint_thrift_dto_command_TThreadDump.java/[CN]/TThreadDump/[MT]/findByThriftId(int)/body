{
switch (fieldId) {
case 1:
    return THREAD_NAME;
case 2:
  return THREAD_ID;
case 3:
return BLOCKED_TIME;
case 4:
return BLOCKED_COUNT;
case 5:
return WAITED_TIME;
case 6:
return WAITED_COUNT;
case 7:
return LOCK_NAME;
case 8:
return LOCK_OWNER_ID;
case 9:
return LOCK_OWNER_NAME;
case 10:
return IN_NATIVE;
case 11:
return SUSPENDED;
case 12:
return THREAD_STATE;
case 13:
return STACK_TRACE;
case 14:
return LOCKED_MONITORS;
case 15:
return LOCKED_SYNCHRONIZERS;
default :
return null;
}
}
