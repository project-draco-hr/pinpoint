{
  if (logger.isDebugEnabled()) {
    logger.debug("Getting common super class. type1={}, type2={}",type1,type2);
  }
  final String className=get(type1,type2);
  if (logger.isDebugEnabled()) {
    logger.debug("Common super class is '{}'. type1={}, type2={}",className,type1,type2);
  }
  return className;
}
