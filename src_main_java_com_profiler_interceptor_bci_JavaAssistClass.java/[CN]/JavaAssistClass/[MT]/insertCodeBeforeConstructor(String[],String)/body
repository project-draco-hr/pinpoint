{
  try {
    CtConstructor constructor=getConstructor(args);
    if (constructor == null) {
      return false;
    }
    constructor.insertBefore(code);
    return true;
  }
 catch (  Exception e) {
    if (logger.isLoggable(Level.WARNING)) {
      logger.log(Level.WARNING,e.getMessage(),e);
    }
    return false;
  }
}
