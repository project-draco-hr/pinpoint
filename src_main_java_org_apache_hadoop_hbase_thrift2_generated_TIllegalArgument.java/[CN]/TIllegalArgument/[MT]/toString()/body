{
  StringBuilder sb=new StringBuilder("TIllegalArgument(");
  boolean first=true;
  if (isSetMessage()) {
    sb.append("message:");
    if (this.message == null) {
      sb.append("null");
    }
 else {
      sb.append(this.message);
    }
    first=false;
  }
  sb.append(")");
  return sb.toString();
}
