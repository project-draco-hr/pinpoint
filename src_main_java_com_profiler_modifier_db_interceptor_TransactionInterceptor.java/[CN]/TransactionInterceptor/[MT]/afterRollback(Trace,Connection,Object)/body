{
  try {
    String connectionUrl=this.getUrl.invoke(target);
    trace.recordRpcName("MYSQL",connectionUrl);
    trace.recordTerminalEndPoint(connectionUrl);
    boolean success=InterceptorUtils.isSuccess(result);
    if (success) {
      trace.recordAttribute("Transaction","rollback");
    }
 else {
      trace.recordAttribute("Transaction","rollback fail");
      Throwable th=(Throwable)result;
      trace.recordAttribute("Exception",th.getMessage());
    }
    trace.record(Annotation.ClientRecv);
  }
 catch (  Exception e) {
    if (logger.isLoggable(Level.WARNING)) {
      logger.log(Level.WARNING,e.getMessage(),e);
    }
  }
 finally {
    trace.traceBlockEnd();
  }
}
