{
  final StringBuilder sb=new StringBuilder();
  sb.append(this.getClass().getSimpleName());
  sb.append("{requestId=").append(getRequestId());
  sb.append(", ");
  if (payload == null) {
    sb.append("payload=null");
  }
 else {
    sb.append("payloadLength=").append(payload.length);
  }
  sb.append('}');
  return sb.toString();
}
