{
  Class clazz=findLoadedClass(name);
  if (clazz == null) {
    if (onLoadClass(name)) {
      clazz=findClass(name);
    }
 else {
      try {
        clazz=parent.loadClass(name);
      }
 catch (      ClassNotFoundException e) {
      }
      if (clazz == null) {
        clazz=findClass(name);
      }
    }
  }
  if (resolve) {
    resolveClass(clazz);
  }
  return clazz;
}
