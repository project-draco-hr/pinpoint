{
  if (discard) {
    limit=checkLimit(span);
    if (!limit) {
synchronized (this) {
        this.storage=null;
      }
      if (isDebug) {
        logger.debug("discard spanEvent");
      }
      dataSender.send(span);
    }
 else {
      flushAll(span);
    }
  }
 else {
    flushAll(span);
  }
}
