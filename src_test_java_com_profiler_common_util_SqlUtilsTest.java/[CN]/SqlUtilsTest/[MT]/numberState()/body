{
  assertEqual("123","#");
  assertEqual("-123","-#");
  assertEqual("+123","+#");
  assertEqual("1.23","#");
  assertEqual("1.23.34","#");
  assertEqual("123 456","# #");
  assertEqual("1.23 4.56","# #");
  assertEqual("1.23-4.56","#-#");
  assertEqual("-- 1.23","-- 1.23");
  assertEqual("--1.23","--1.23");
  assertEqual("/* 1.23 */","/* 1.23 */");
  assertEqual("/*1.23*/","/*1.23*/");
  assertEqual("/* 1.23 \n*/","/* 1.23 \n*/");
  assertEqual("test123","test123");
  assertEqual("123tst","#tst");
}
