{
  if (isDebug) {
    logger.beforeInterceptor(target,args);
  }
  final Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    return;
  }
  if (!validate(target)) {
    return;
  }
  SpanEventRecorder recorder=trace.traceBlockBegin();
  recorder.recordServiceType(OK_HTTP_CLIENT_INTERNAL);
}
