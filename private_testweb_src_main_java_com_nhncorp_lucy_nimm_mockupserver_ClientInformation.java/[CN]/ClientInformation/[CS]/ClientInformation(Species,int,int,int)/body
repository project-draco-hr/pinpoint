{
  this.domainId=domainId;
  this.idcId=idcId;
  this.serverId=serverId;
  this.domainIdSet=Collections.synchronizedSet(new TreeSet<Integer>());
  try {
    this.representativeAddress=addressAdmin.retrieveAddressInstance(species,0,this.idcId,this.serverId,0,0);
