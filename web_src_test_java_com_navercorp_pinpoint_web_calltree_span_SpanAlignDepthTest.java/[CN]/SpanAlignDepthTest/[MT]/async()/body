{
  expectResult.clear();
  expectResult.add("#");
  expectResult.add("##");
  expectResult.add("#");
  callTree.clear();
  callTree.add(makeSpanEventBo(SYNC,(short)0,1));
  callTree.add(makeSpanEventBo(ASYNC,(short)0,1));
  callTree.add(makeSpanEventBo(SYNC,(short)1,-1));
  assertDepth(callTree,expectResult);
  expectResult.clear();
  expectResult.add("#");
  expectResult.add("##");
  expectResult.add("###");
  expectResult.add("####");
  callTree.clear();
  callTree.add(makeSpanEventBo(SYNC,(short)0,1));
  callTree.add(makeSpanEventBo(ASYNC,(short)0,1));
  callTree.add(makeSpanEventBo(ASYNC,(short)1,2));
  callTree.add(makeSpanEventBo(ASYNC,(short)0,1));
  assertDepth(callTree,expectResult);
  expectResult.clear();
  expectResult.add("#");
  expectResult.add("##");
  expectResult.add("###");
  expectResult.add("#");
  callTree.clear();
  callTree.add(makeSpanEventBo(SYNC,(short)0,1));
  callTree.add(makeSpanEventBo(ASYNC,(short)0,1));
  callTree.add(makeSpanEventBo(ASYNC,(short)0,1));
  callTree.add(makeSpanEventBo(SYNC,(short)1,-1));
  assertDepth(callTree,expectResult);
}
