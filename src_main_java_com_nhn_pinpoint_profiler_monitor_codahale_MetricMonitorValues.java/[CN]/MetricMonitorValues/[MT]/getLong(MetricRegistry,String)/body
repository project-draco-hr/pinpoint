{
  SortedMap<String,Gauge> gauges=registry.getGauges();
  Gauge gauge=gauges.get(key);
  if (gauge == null) {
    return 0;
  }
  Object value=gauge.getValue();
  if (value == null) {
    return 0;
  }
 else   if (value instanceof Long || value instanceof Integer) {
    return (Long)value;
  }
 else {
    return 0;
  }
}
