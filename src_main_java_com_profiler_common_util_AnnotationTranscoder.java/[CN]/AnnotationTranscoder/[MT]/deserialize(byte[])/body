{
  Object rv=null;
  try {
    if (in != null) {
      ByteArrayInputStream bis=new ByteArrayInputStream(in);
      ObjectInputStream is=new ObjectInputStream(bis);
      rv=is.readObject();
      is.close();
      bis.close();
    }
  }
 catch (  IOException e) {
  }
catch (  ClassNotFoundException e) {
  }
  return rv;
}
