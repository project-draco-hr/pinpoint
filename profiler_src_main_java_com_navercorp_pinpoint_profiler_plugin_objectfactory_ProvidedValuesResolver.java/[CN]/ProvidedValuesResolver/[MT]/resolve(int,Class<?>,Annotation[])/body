{
  if (this.index >= values.length) {
    return Option.<Object>empty();
  }
  Object candidate=values[this.index];
  if (type.isPrimitive()) {
    if (candidate == null) {
      return Option.<Object>empty();
    }
    if (TypeUtils.getWrapperOf(type) == candidate.getClass()) {
      this.index++;
      return Option.withValue(candidate);
    }
  }
 else {
    if (type.isInstance(candidate)) {
      this.index++;
      return Option.withValue(candidate);
    }
  }
  return Option.<Object>empty();
}
