{
  if (isDebug) {
    logger.beforeInterceptor(target,args);
  }
  Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    if (isDebug) {
      logger.debug("TraceID not exist. start new trace.");
    }
    trace=traceContext.newTraceObject();
  }
  trace.traceBlockBegin();
  trace.markBeforeTime();
}
