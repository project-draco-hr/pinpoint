{
  PageContext pageContext=(PageContext)getJspContext();
  JspWriter out=pageContext.getOut();
  try {
    StringBuffer sb=new StringBuffer();
    sb.append("<link rel=\"");
    sb.append(rel);
    sb.append("\" href=\"");
    sb.append(href);
    sb.append("\">");
    out.println(sb.toString());
  }
 catch (  Exception e) {
    logger.error(e.getMessage(),e);
  }
}
