{
  if (isDebug) {
    logger.afterInterceptor(target,args,result);
  }
  if (JDBCScope.isInternal()) {
    return;
  }
  Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    return;
  }
  try {
    trace.recordException(result);
    trace.markAfterTime();
  }
  finally {
    trace.traceBlockEnd();
  }
}
