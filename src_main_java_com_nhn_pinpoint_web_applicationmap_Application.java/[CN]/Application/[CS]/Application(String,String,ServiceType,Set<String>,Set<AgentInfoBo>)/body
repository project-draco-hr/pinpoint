{
  this.id=id;
  if (serviceType == ServiceType.CLIENT) {
    this.applicationName="CLIENT";
  }
 else {
    this.applicationName=applicationName;
  }
  if (hosts != null) {
    this.hosts.addAll(hosts);
  }
  if (agents != null) {
    this.agents.addAll(agents);
  }
  this.serviceType=serviceType;
}
