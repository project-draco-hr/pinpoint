{
  Object temp=null;
  if ((temp=prop.get("SERVER_TCP_LISTEN_PORT")) != null)   SERVER_TCP_LISTEN_PORT=Integer.parseInt(temp.toString());
  if ((temp=prop.get("SERVER_UDP_LISTEN_PORT")) != null)   SERVER_UDP_LISTEN_PORT=Integer.parseInt(temp.toString());
  logger.info("##### Hippo Config Loaded successfully. #####");
}
