{
  if (source == null) {
    throw new NullPointerException("source must not be null");
  }
  final NodeId id=source.getId();
  final Application find=nodeMap.get(id);
  if (find != null) {
    find.add(source);
  }
 else {
    final Application application=source.deepCopy();
    nodeMap.put(id,application);
  }
}
