{
  StringBuilder sb=new StringBuilder("Endpoint(");
  boolean first=true;
  sb.append("ip:");
  if (this.ip == null) {
    sb.append("null");
  }
 else {
    sb.append(this.ip);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("port:");
  sb.append(this.port);
  first=false;
  if (!first)   sb.append(", ");
  sb.append("name:");
  if (this.name == null) {
    sb.append("null");
  }
 else {
    sb.append(this.name);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}
