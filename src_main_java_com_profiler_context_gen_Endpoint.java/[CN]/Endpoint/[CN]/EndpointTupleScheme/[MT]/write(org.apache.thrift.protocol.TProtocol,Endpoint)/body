{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetIp()) {
    optionals.set(0);
  }
  if (struct.isSetPort()) {
    optionals.set(1);
  }
  if (struct.isSetName()) {
    optionals.set(2);
  }
  oprot.writeBitSet(optionals,3);
  if (struct.isSetIp()) {
    oprot.writeString(struct.ip);
  }
  if (struct.isSetPort()) {
    oprot.writeI16(struct.port);
  }
  if (struct.isSetName()) {
    oprot.writeString(struct.name);
  }
}
