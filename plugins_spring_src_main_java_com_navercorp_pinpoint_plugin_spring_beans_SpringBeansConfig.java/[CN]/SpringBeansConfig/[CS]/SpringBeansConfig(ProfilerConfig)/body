{
  boolean enable=config.readBoolean(ENABLE,true);
  if (!enable) {
    if (logger.isInfoEnabled()) {
      logger.info("Disable spring-beans plugin");
    }
    return;
  }
  addBackwardCompatibilityTarget(config);
  addTarget(config);
  if (logger.isInfoEnabled()) {
    logger.info("Add spring-beans targets {}",targets);
  }
}
