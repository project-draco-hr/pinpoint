{
  try {
    TraceID nextId=getNextTraceId();
    callStack.push();
    StackFrame stackFrame=createCallInfo(nextId,HANDLER_STACKID);
    callStack.setStackFrame(stackFrame);
    handler.handle(nextId);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    callStack.pop();
  }
}
