{
  try {
    if (isDebug) {
      logger.fine("[WRITE SubSPAN]" + subSpan + " CurrentThreadID="+ Thread.currentThread().getId()+ ",\n\t CurrentThreadName="+ Thread.currentThread().getName()+ "\n\n");
    }
    this.storage.store(subSpan);
  }
 catch (  Exception e) {
    logger.log(Level.SEVERE,e.getMessage(),e);
  }
}
