{
  TraceIDStack stack=traceIdLocal.get();
  TraceID traceId=null;
  if (stack != null)   traceId=stack.getTraceId();
  if (traceId != null) {
    if (stack == null) {
      traceIdLocal.set(new TraceIDStack());
    }
    traceIdLocal.get().clear();
    spanMap.remove(traceId);
    return true;
  }
  return false;
}
