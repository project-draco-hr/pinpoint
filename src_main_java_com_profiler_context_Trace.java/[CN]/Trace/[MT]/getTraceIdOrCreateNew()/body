{
  TraceIDStack stack=traceIdLocal.get();
  TraceID id=null;
  if (stack != null) {
    id=stack.getTraceId();
  }
  if (id == null) {
    System.out.println("create new traceid");
    id=TraceID.newTraceId();
    if (stack == null) {
      traceIdLocal.set(new TraceIDStack());
    }
    traceIdLocal.get().setTraceId(id);
    return id;
  }
  return id;
}
