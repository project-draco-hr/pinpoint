{
  final PinpointSocketFactory pinpointSocketFactory=new PinpointSocketFactory();
  pinpointSocketFactory.setReconnectDelay(200);
  PinpointSocket socket=pinpointSocketFactory.scheduledConnect("localhost",10234);
  Thread.sleep(2000);
  logger.debug("close");
  socket.close();
  Thread.sleep(1000);
}
