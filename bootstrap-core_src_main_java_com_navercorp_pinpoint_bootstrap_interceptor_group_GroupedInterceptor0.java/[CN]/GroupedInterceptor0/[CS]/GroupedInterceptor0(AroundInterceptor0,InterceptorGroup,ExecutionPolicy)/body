{
  if (interceptor == null) {
    throw new NullPointerException("interceptor must not be null");
  }
  if (group == null) {
    throw new NullPointerException("group must not be null");
  }
  if (policy == null) {
    throw new NullPointerException("policy must not be null");
  }
  this.interceptor=interceptor;
  this.group=group;
  this.policy=policy;
}
