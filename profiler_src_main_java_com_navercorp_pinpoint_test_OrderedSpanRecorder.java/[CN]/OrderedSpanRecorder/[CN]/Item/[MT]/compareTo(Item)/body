{
  if (this.time < o.time) {
    return -1;
  }
 else   if (this.time > o.time) {
    return 1;
  }
 else {
    if (this.spanId < o.spanId) {
      return -1;
    }
 else     if (this.spanId > o.spanId) {
      return 1;
    }
 else {
      return this.sequence < o.sequence ? -1 : (this.sequence > o.sequence ? 1 : 0);
    }
  }
}
