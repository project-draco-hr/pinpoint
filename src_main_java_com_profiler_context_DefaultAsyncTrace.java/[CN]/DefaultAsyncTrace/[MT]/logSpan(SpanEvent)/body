{
  try {
    if (isTrace) {
      Thread thread=Thread.currentThread();
      logger.trace("[WRITE SpanEvent]" + spanEvent + " CurrentThreadID="+ thread.getId()+ ",\n\t CurrentThreadName="+ thread.getName());
    }
    this.storage.store(spanEvent);
  }
 catch (  Exception e) {
    logger.warn(e.getMessage(),e);
  }
}
