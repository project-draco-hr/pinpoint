{
  StringBuilder builder=new StringBuilder();
  builder.append('(');
  builder.append(span.getServiceType());
  builder.append(", [");
  boolean first=true;
  for (  TAnnotation a : span.getAnnotations()) {
    if (first) {
      first=false;
    }
 else {
      builder.append(", ");
    }
    builder.append(toString(a));
  }
  builder.append("])");
  return builder.toString();
}
