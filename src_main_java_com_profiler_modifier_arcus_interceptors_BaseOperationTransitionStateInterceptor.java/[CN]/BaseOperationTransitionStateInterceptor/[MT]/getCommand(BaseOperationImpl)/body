{
  ByteBuffer buffer=baseOperation.getBuffer();
  if (buffer == null) {
    return "UNKNOWN";
  }
  System.out.println(buffer.array().length + " po:" + buffer.position()+ " limit:"+ buffer.limit()+ " remaining"+ buffer.remaining()+ " aoffset:"+ buffer.arrayOffset());
  return new String(buffer.array(),UTF8);
}
