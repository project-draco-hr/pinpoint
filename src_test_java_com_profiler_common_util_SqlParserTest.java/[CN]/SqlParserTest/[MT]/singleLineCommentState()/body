{
  assertEqual("--","--","");
  assertEqual("//","//");
  assertEqual("--123","--123");
  assertEqual("//123","//123");
  assertEqual("--test","--test");
  assertEqual("//test","//test");
  assertEqual("--test\ntest","--test\ntest");
  assertEqual("--test\t\n","--test\t\n");
  assertEqual("--test\n123 test","--test\n# test");
}
