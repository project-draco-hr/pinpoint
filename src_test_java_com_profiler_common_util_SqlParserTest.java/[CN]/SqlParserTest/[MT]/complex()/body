{
  assertEqual("select * from table a = 1 and b=50 and c=? and d='11'","select * from table a = # and b=# and c=? and d='$'");
  assertEqual("select * from table a = -1 and b=-50 and c=? and d='-11'","select * from table a = -# and b=-# and c=? and d='$'");
  assertEqual("select * from table a = 1/*test*/ and b=50/*test*/ and c=? and d='11'","select * from table a = #/*test*/ and b=#/*test*/ and c=? and d='$'");
  assertEqual("select ZIPCODE,CITY from ZIPCODE");
  assertEqual("select a.ZIPCODE,a.CITY from ZIPCODE as a");
  assertEqual("select ZIPCODE,123 from ZIPCODE","select ZIPCODE,# from ZIPCODE");
  assertEqual("SELECT * from table a=123 and b='abc' and c=1-3","SELECT * from table a=# and b='$' and c=#-#");
  assertEqual("SYSTEM_RANGE(1, 10)","SYSTEM_RANGE(#, #)");
}
