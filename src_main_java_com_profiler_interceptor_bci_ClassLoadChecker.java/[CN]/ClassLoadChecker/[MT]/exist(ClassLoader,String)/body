{
  LoadClass key=new LoadClass(cl,className);
  Object old=load.putIfAbsent(key,EXIST);
  if (old == null) {
    if (logger.isInfoEnabled()) {
      logger.info("{} not exist from ",cl);
    }
    return false;
  }
  if (logger.isInfoEnabled()) {
    logger.info("{} already exist from ",cl);
  }
  return true;
}
