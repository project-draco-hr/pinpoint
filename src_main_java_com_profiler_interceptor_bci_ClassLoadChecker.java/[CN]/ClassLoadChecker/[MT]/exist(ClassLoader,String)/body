{
  LoadClass key=new LoadClass(cl,className);
  Object old=load.putIfAbsent(key,EXIST);
  if (old == null) {
    if (logger.isLoggable(Level.INFO)) {
      logger.info(className + " not exist from " + cl);
    }
    return false;
  }
  if (logger.isLoggable(Level.INFO)) {
    logger.info(className + " already exist from " + cl);
  }
  return true;
}
