{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetHeader()) {
    optionals.set(0);
  }
  if (struct.isSetRequest()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetHeader()) {
    struct.header.write(oprot);
  }
  if (struct.isSetRequest()) {
    struct.request.write(oprot);
  }
}
