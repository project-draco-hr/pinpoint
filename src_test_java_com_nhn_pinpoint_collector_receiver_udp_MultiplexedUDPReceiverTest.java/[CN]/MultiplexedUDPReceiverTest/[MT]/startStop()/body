{
  try {
    GenericApplicationContext context=ApplicationContextUtils.createContext();
    DataReceiver receiver=new MultiplexedUDPReceiver(context);
    Future<Boolean> startLatch=receiver.start();
    startLatch.get();
    receiver.shutdown();
    context.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
    Assert.fail(e.getMessage());
  }
}
