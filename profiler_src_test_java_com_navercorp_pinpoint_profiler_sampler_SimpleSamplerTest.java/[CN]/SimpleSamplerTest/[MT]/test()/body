{
  SamplingRateSampler simpleSampler=new SamplingRateSampler(1);
  assertChoice(simpleSampler);
  assertChoice(simpleSampler);
  assertChoice(simpleSampler);
  assertChoice(simpleSampler);
  SamplingRateSampler simpleSampler2=new SamplingRateSampler(2);
  assertChoice(simpleSampler2);
  assertDrop(simpleSampler2);
  assertChoice(simpleSampler2);
  assertDrop(simpleSampler2);
}
