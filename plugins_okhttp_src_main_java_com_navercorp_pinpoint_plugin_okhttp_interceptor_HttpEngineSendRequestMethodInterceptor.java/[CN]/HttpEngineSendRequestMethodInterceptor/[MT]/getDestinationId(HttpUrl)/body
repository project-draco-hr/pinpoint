{
  if (httpUrl == null || httpUrl.host() == null) {
    return "UnknownHttpClient";
  }
  if (httpUrl.port() == HttpUrl.defaultPort(httpUrl.scheme())) {
    return httpUrl.host();
  }
  final StringBuilder sb=new StringBuilder();
  sb.append(httpUrl.host());
  sb.append(':');
  sb.append(httpUrl.port());
  return sb.toString();
}
