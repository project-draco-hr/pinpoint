{
  if (!enabled) {
    if (getLogger().isDebugEnabled()) {
      getLogger().debug("interceptor is disabled.");
    }
    return;
  }
  if (getLogger().isDebugEnabled()) {
    getLogger().beforeInterceptor(target,args);
  }
  Trace trace=traceContext.currentTraceObject();
  if (trace == null) {
    return;
  }
  trace.traceBlockBegin();
  trace.markBeforeTime();
}
