{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.histogram != null) {
    if (struct.isSetHistogram()) {
      oprot.writeFieldBegin(HISTOGRAM_FIELD_DESC);
      struct.histogram.write(oprot);
      oprot.writeFieldEnd();
    }
  }
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}
